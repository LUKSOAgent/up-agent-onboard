{
  "version": 2,
  "builds": [
    {
      "src": "**/*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/human",
      "dest": "/human/index.html"
    },
    {
      "src": "/agent",
      "dest": "/agent/index.html"
    },
    {
      "src": "/human/(.*)",
      "dest": "/human/index.html"
    },
    {
      "src": "/agent/(.*)",
      "dest": "/agent/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
